{
  "version": 2,
  "name": "Birth Wish Hub",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "uploads/**",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public/uploads"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}