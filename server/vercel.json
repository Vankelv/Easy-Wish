{
  "version": 2,
  "name": "Birth Wish Hub",
  "cors": {
    "*": true
  },
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "uploads/$1"
    }

  ]
  
}